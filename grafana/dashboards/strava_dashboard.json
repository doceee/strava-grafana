{
  "id": null,
  "title": "Strava Activities",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 10,
  "refresh": "5m",
  "panels": [
    {
      "type": "timeseries",
      "title": "Distance per Activity (km)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 9 },
      "targets": [
        {
          "refId": "A",
          "datasource": {
            "type": "mysql",
            "uid": "dashboard-strava"
          },
          "format": "time_series",
          "rawSql": "SELECT UNIX_TIMESTAMP(start_date) as time, distance/1000 as value FROM strava_activities WHERE $__timeFilter(start_date) ORDER BY start_date",
          "intervalMs": 1000,
          "maxDataPoints": 1000
        }
      ]
    },
    {
      "type": "table",
      "title": "All Activities",
      "gridPos": { "x": 0, "y": 9, "w": 24, "h": 10 },
      "targets": [
        {
          "refId": "D",
          "datasource": { "type": "mysql", "uid": "dashboard-strava" },
          "format": "table",
          "rawSql": "SELECT start_date, name, type, distance/1000 AS distance_km, moving_time/60 AS moving_time_min, elapsed_time/60 AS elapsed_time_min, total_elevation_gain AS elevation_gain_m FROM strava_activities ORDER BY start_date DESC"
        }
      ],
      "options": {
        "sortBy": [
          {
            "displayName": "start_date",
            "desc": true
          }
        ]
      }
    }
  ],
  "templating": {
    "list": []
  }
}
